
public class TriangularNumber {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 0;
		int y = 0;
		//i=0;
		while(y<7){
			++y;
			x = x + y;
			for(int i =1; i <= x; i++){
				if(x % i == 0)
					System.out.println(x);
			}
			
		}
		System.out.print(x);

	}

}
